{
  "name": "yar-carousel",
  "version": "1.0.3",
  "description": "Yet Another React Carousel",
  "author": {
    "name": "Gianluca Candiotti",
    "url": "gianluca.prwlr@gmail.com"
  },
  "main": "dist/js/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GianlucaCandiotti/yar-carousel"
  },
  "bugs": {
    "url": "https://github.com/GianlucaCandiotti/yar-carousel/issues"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "transform-class-properties"
          ]
        }
      ]
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start-dev": "parallelshell 'npm run watch' 'npm run sync'",
    "preclean": "bin/preclean",
    "clean": "bin/clean",
    "preclean-dist": "bin/preclean-dist",
    "clean-dist": "bin/clean-dist",
    "sync": "browser-sync start --server --files 'build/**/*.css, build/**/*.js'",
    "lint-js": "bin/lint-js",
    "build-vendor-js": "bin/build-vendor-js",
    "build-js": "bin/build-js",
    "prep-dist-js": "bin/prep-dist-js",
    "watch-js": "bin/watch-js",
    "build-css": "bin/build-css",
    "prep-dist-css": "bin/prep-dist-css",
    "watch-css": "bin/watch-css",
    "build": "npm run clean && npm run build-vendor-js && npm run build-js && npm run build-css",
    "prep-dist": "npm run clean-dist && npm run prep-dist-js && npm run prep-dist-css",
    "watch": "parallelshell 'npm run watch-js' 'npm run watch-css'"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "swipe-js-iso": "^2.0.2",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react-easy-swipe": "^0.0.4"
  },
  "keywords": [
    "react",
    "react-component",
    "carousel",
    "slider"
  ]
}
